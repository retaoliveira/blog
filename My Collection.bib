number = {2018},
pages = {370--380},
publisher = {Elsevier B.V.},
title = {{Challenges to urban freight transport in historical cities: A case study for Sabar{\'{a}} (Brazil)}},
url = {https://doi.org/10.1016/j.trpro.2019.06.039},
volume = {39},
