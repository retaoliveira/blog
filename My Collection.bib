issn = {2237-1346},
journal = {Transportes},
keywords = {frequency seong problem,transit network design},
number = {1},
pages = {142--156},
title = {{Restri{\c{c}}{\~{o}}es de carga urbana e desafios log{\'{i}}sticos: Percep{\c{c}}{\~{a}}o de varejistas e motoristas em Sorocaba}},
