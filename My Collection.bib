author = {{De Oliveira}, Leise Kelli and Stubbs, Luciana Carneiro de Morais and Gontijo, Nebai Tavares and de Oliveira, Renata L{\'{u}}cia Magalh{\~{a}}es},
doi = {10.14295/transportes.v25i2.1243},
file = {:C$\backslash$:/Users/user/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/De Oliveira et al. - 2017 - Proposi{\c{c}}{\~{a}}o de modelos de gera{\c{c}}{\~{a}}o de viagens para Belo Horizonte.pdf:pdf},
