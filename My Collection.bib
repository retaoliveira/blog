file = {:C$\backslash$:/Users/user/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/De Oliveira et al. - 2019 - Analysis of accessibility from collection and delivery points Towards the sustainability of the e-commerce d.pdf:pdf},
